<form method="POST">
    <label for="email">Email:</label>
    <input type="email" id="email" name="email">
    <span class="error"><%- include('partials/errors', {errorType:"email"}) %></span>
    <label for "password">Password:</label>
    <input type="password" id="password" name="password">
    <span class="error"><%- include('partials/errors', {errorType:"password"}) %></span>
    <div class="button-container">
        <button class="clickable" type="submit">Log-in</button>
        <a class="clickable" href="./..">Cancel</a> 
    </div>
</form>